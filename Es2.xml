<!-- corpo del documento XML-->
<elementoRadice>
    <elementoFiglio>
        <elementoAnnidato>
        </elementoAnnidato>
    </elementoFiglio>
    <elementoFiglio>
        <elementoAnnidato>
            <elementoAnnidato>
                <elementoFiglio>
<!-- Errore: prima di inserire il tag di chiusura dell'elemento <elementoAnnidato> 
             dev'essere inserito il tag di chiusura dell'elemento <ElementoFiglio>-->
                </elementoFiglio>
            </elementoAnnidato>
<!-- Errore: l'elemento <elementoFiglio> è già stato chiuso correttamente prima
             adesso si deve inserire il tag di chiusura dell'elemento <elementoAnnidato>-->
        </elementoAnnidato>
        <elementoAnnidato>
            <elementoFiglio>
            </elementoFiglio>
<!-- Errore: è necessario inserire il tag di chiusura dell'elemento <elementoAnnidato>-->
        </elementoAnnidato>
    </elementoFiglio>
</elementoRadice>

<!-- epilogo -->